{
  "CONFIG_VERSION_DONT_TOUCH_THIS": 1,
  "_comment": "Before changing anything, see https://github.com/Patbox/StyledNicknames#configuration",
  "allowByDefault": true,
  "nicknameFormat": "~${nickname}",
  "maxLength": 32,
  "changeDisplayName": true,
  "changePlayerListName": false,
  "allowLegacyFormatting": true,
  "nicknameChangedMessage": "<gold>Your nickname is now</gold> ${nickname}<gold>.</gold>",
  "nicknameResetMessage": "<gold>You no longer have a nickname.</gold>",
  "defaultEnabledFormatting": {
    "aqua": true,
    "black": false,
    "blue": true,
    "bold": false,
    "change_page": false,
    "color": true,
    "dark_aqua": true,
    "dark_blue": true,
    "dark_gray": true,
    "dark_green": true,
    "dark_purple": true,
    "dark_red": true,
    "gold": true,
    "gradient": true,
    "gray": true,
    "green": true,
    "hard_gradient": true,
    "hover": false,
    "italic": false,
    "light_purple": true,
    "obfuscated": false,
    "rainbow": true,
    "red": true,
    "reset": true,
    "strikethrough": false,
    "underline": false,
    "white": true,
    "yellow": true
  },
  "tooLongMessage": "<dark_red>Error:</dark_red> <red>That nickname is too long.</red>"
}